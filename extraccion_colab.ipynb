    "# --- CONFIGURACI√ìN PRINCIPAL ---",
    "URL_SCRAPING = \"https://live20.nowgoal25.com/\"",
    "GOOGLE_SHEET_NAME = 'Almacen_Stre'",
    "UPCOMING_SHEET_NAME = 'Hoja 1'",
    "\n",
    "# --- INICIO DEL PROCESO ---",
    "print(\"üöÄ Iniciando el proceso de scraping y carga...")",
    "driver = None",
    "try:",
    "    # 1. Obtener contenido HTML",
    "    print(\"Paso 1: Obteniendo contenido de la p√°gina...")",
    "    html_content = fetch_page_content_with_requests(URL_SCRAPING)",
    "    if not html_content:",
    "        print(\"  ‚ö†Ô∏è Requests fall√≥. Intentando con Selenium (puede tardar un poco)...")",
    "        driver = setup_driver_colab()",
    "        html_content = fetch_page_content_with_selenium(driver, URL_SCRAPING)",
    "    ",
    "    if not html_content:",
    "        print(\"üõë ERROR: No se pudo obtener el contenido de la p√°gina.")",
    "    else:",
    "        print(\"  ‚úÖ Contenido HTML obtenido.")",
    "        ",
    "        # 2. Parsear datos a DataFrame",
    "        print(\"Paso 2: Parseando los datos de los partidos...")",
    "        upcoming_df = parse_match_data_to_dataframe(html_content)",
    "        if upcoming_df.empty:",
    "            print(\"  ‚ö†Ô∏è No se encontraron partidos pr√≥ximos.")",
    "        else:",
    "            print(f\"  ‚úÖ Se encontraron {len(upcoming_df)} partidos pr√≥ximos.")",
    "            ",
    "            # 3. Subir a Google Sheets",
    "            print(\"Paso 3: Subiendo datos a Google Sheets...")",
    "            try:",
    "                spreadsheet = gc.open(GOOGLE_SHEET_NAME)",
    "                print(f\"  ‚úÖ Abierto el Google Sheet '{GOOGLE_SHEET_NAME}'.")",
    "                actualizar_hoja_completa(spreadsheet, UPCOMING_SHEET_NAME, upcoming_df)",
    "            except gspread.exceptions.SpreadsheetNotFound:",
    "                print(f\"üõë ERROR: El Google Sheet '{GOOGLE_SHEET_NAME}' no fue encontrado. Aseg√∫rate de que existe y lo has compartido con el email de servicio: {gc.auth.service_account_email}")",
    "            except Exception as e:",
    "                print(f\"üõë ERROR durante la interacci√≥n con Google Sheets: {e}")",
    "finally:",
    "    if driver:",
    "        driver.quit()",
    "    print("\nüèÅ Proceso finalizado.")"
   ]
