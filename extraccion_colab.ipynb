    "import datetime\n",
    "import time\n",
    "import sys\n",
    "import pytz\n",
    "import pandas as pd\n",
    "from bs4 import BeautifulSoup\n",
    "import requests\n",
    "from requests.adapters import HTTPAdapter\n",
    "from urllib3.util.retry import Retry\n",
    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "from selenium.webdriver.support.ui import WebDriverWait\n",
    "from selenium.webdriver.support import expected_conditions as EC\n",
    "from selenium.common.exceptions import TimeoutException, WebDriverException\n",
    "from gspread_dataframe import set_with_dataframe\n",
    "\n",
    "MADRID_TZ = pytz.timezone('Europe/Madrid')\n",
    "\n",
    "def get_requests_session():\n",
    "    session = requests.Session()\n",
    "    retries = Retry(total=3, backoff_factor=0.5, status_forcelist=[500, 502, 503, 504])\n",
    "    adapter = HTTPAdapter(max_retries=retries)\n",
    "    session.mount(\"https://\", adapter)\n",
    "    session.mount(\"http://\", adapter)\n",
    "    session.headers.update({\"User-Agent\": \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 Chrome/116.0.0.0 Safari/537.36\"})\n",
    "    return session\n",
    "\n",
    "def setup_driver_colab():\n",
    "    options = webdriver.ChromeOptions()\n",
    "    options.add_argument(\"--headless\")\n",
    "    options.add_argument(\"--no-sandbox\")\n",
    "    options.add_argument(\"--disable-dev-shm-usage\")\n",
    "    options.add_argument(\"--disable-gpu\")\n",
    "    options.add_argument(\"user-agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 Chrome/116.0.0.0 Safari/537.36\")\n",
    "    options.add_argument(\"\'--blink-settings=imagesEnabled=false'\")\n",
    "    try:\n",
    "        driver = webdriver.Chrome(options=options)\n",
    "        return driver\n",
    "    except WebDriverException as e:\n",
    "        print(f\"Error inicializando Selenium driver: {e}\")\n",
    "        return None\n",
    "\n",
    "def fetch_page_content_with_selenium(driver, url):\n",
    "    if not driver:\n",
    "        print(\"No se pudo iniciar el navegador.\")\n",
    "        return None\n",
    "    try:\n",
    "        driver.get(url)\n",
    "        WebDriverWait(driver, 30).until(EC.presence_of_element_located((By.CSS_SELECTOR, \"tr[id^='tr1_']\")))\n",
    "        time.sleep(2)\n",
    "        return driver.page_source\n",
    "    except TimeoutException:\n",
    "        print(\"Tiempo de espera agotado esperando el contenido de la p√°gina.\")\n",
    "        return None\n",
    "    except Exception as e:\n",
    "        print(f\"Ocurri√≥ un error al cargar la p√°gina con Selenium: {e}\")\n",
    "        return None\n",
    "\n",
    "def fetch_page_content_with_requests(url):\n",
    "    try:\n",
    "        session = get_requests_session()\n",
    "        response = session.get(url, timeout=10)\n",
    "        response.raise_for_status()\n",
    "        return response.text\n",
    "    except Exception as e:\n",
    "        print(f\"Ocurri√≥ un error al cargar la p√°gina con requests: {e}\")\n",
    "        return None\n",
    "\n",
    "def parse_match_data_to_dataframe(html_content):\n",
    "    soup = BeautifulSoup(html_content, 'html.parser')\n",
    "    match_rows = soup.find_all('tr', id=lambda x: x and x.startswith('tr1_'))\n",
    "    upcoming_matches = []\n",
    "    now_madrid = datetime.datetime.now(MADRID_TZ)\n",
    "\n",
    "    for row in match_rows:\n",
    "        match_id = row.get('id', '').replace('tr1_', '')\n",
    "        if not match_id: continue\n",
    "\n",
    "        time_cell = row.find('td', {'name': 'timeData'})\n",
    "        if not time_cell or not time_cell.has_attr('data-t'): continue\n",
    "        \n",
    "        try:\n",
    "            match_time_utc = datetime.datetime.strptime(time_cell['data-t'], '%Y-%m-%d %H:%M:%S')\n",
    "            match_time_utc = pytz.utc.localize(match_time_utc)\n",
    "            match_time_madrid = match_time_utc.astimezone(MADRID_TZ)\n",
    "        except (ValueError, IndexError):\n",
    "            continue\n",
    "\n",
    "        if match_time_madrid < now_madrid: continue\n",
    "\n",
    "        home_team_tag = row.find('a', {'id': f'team1_{match_id}'})\n",
    "        away_team_tag = row.find('a', {'id': f'team2_{match_id}'})\n",
    "        odds_data = row.get('odds', '').split(',')\n",
    "        league_cell = row.find('td', {'name': 'leagueData'})\n",
    "\n",
    "        handicap = odds_data[2] if len(odds_data) > 2 else ""\n",
    "        goal_line = odds_data[10] if len(odds_data) > 10 else ""\n",
    "\n",
    "        if not handicap or not goal_line: continue\n",
    "\n",
    "        upcoming_matches.append({\n",
    "            "id": match_id,\n",
    "            "time_utc": match_time_utc.strftime('%Y-%m-%d %H:%M'),\n",
    "            "time_madrid": match_time_madrid.strftime('%Y-%m-%d %H:%M'),\n",
    "            "home_team": home_team_tag.text.strip() if home_team_tag else "",\n",
    "            "away_team": away_team_tag.text.strip() if away_team_tag else "",\n",
    "            "handicap": handicap,\n",
    "            "goal_line": goal_line,\n",
    "            "league": league_cell.text.strip() if league_cell else ""\n",
    "        })\n",
    "\n",
    "    df = pd.DataFrame(upcoming_matches)\n",
    "    if not df.empty:\n",
    "        df = df.sort_values(by='time_utc').head(20)\n",
    "    return df\n",
    "\n",
    "def actualizar_hoja_completa(spreadsheet, sheet_name, dataframe):\n",
    "    try:\n",
    "        worksheet = spreadsheet.worksheet(sheet_name)\n",
    "        worksheet.clear()\n",
    "        if not dataframe.empty:\n",
    "            set_with_dataframe(worksheet, dataframe, resize=True)\n",
    "        print(f\"‚úÖ Hoja '{sheet_name}' actualizada correctamente con {len(dataframe)} filas.\")\n",
    "    except gspread.exceptions.WorksheetNotFound:\n",
    "        print(f\"üõë ERROR: No se encontr√≥ la hoja con el nombre '{sheet_name}'. Creando una nueva...\")\n",
    "        worksheet = spreadsheet.add_worksheet(title=sheet_name, rows=100, cols=20)\n",
    "        if not dataframe.empty:\n",
    "            set_with_dataframe(worksheet, dataframe, resize=True)\n",
    "        print(f\"‚úÖ Hoja '{sheet_name}' creada y actualizada.\")\n",
    "    except Exception as e:\n",
    "        print(f\"üõë Ocurri√≥ un error al actualizar la hoja '{sheet_name}': {e}\")"
   ]
